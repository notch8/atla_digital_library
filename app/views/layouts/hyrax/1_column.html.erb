<!-- Override file from Hyrax 2.3.3 -->
<!DOCTYPE html>
<html lang="<%= t("hyrax.document_language") %>" prefix="og:http://ogp.me/ns#">
  <head>
    <%= render partial: 'layouts/head_tag_content' %>
  </head>
  <% content_for(:extra_body_classes, 'frontend') %>
  <body class="<%= body_class %>">
    <div id='no-home'>
      <a href="#skip_to_content" class="sr-only">Skip to Content</a>
      <%= render partial: '/masthead' %>
      <div id="content-wrapper" class="one-pages">
        <%= render partial: '/flash_msg' %>
        <div class="container">
          <%= render_breadcrumbs builder: Hyrax::BootstrapBreadcrumbsBuilder %>
        </div>
        <% if content_for?(:page_header) %>
          <div class="row">
            <div class="col-xs-12 main-header">
              <%= yield(:page_header) %>
            </div>
          </div>
        <% end %>

        <a name="skip_to_content"></a>
        <div class="row">
          <div class="container">
            <%= yield %>
          </div>
        </div><!-- /.row -->
      </div><!-- /#content-wrapper -->
      <%= render 'shared/footer' %>
      <%= render 'shared/ajax_modal' %>
    </div>
  </body>
</html>
