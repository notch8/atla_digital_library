<!-- Override file from Hyrax 2.3.3 -->
<!-- Removed default navbar inline styling and background image divs -->
<% content_for(:precontainer_content) do %>
  <%= render 'hyrax/homepage/announcement' if controller_name == 'homepage' %>

<% end %>

<% content_for(:content) do %>

  <section class="section-white">
    <div class="container">
      <div class="row">
        <div id="content-wrapper" class="content-wrapper-homepage">
          <%= yield %>
        </div><!-- /#content-wrapper -->
      </div>
    </div>
  </section>
  <!-- END YIELD CONTENT -->
<% end %>

<%= render template: 'layouts/hyrax' %>
