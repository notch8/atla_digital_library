<!-- Removed badges -->
<% presenter.title.each_with_index do |title, index| %>
  <div class="row">
    <div class=<%= presenter.editor? ? "col-sm-8" : "col-sm-12" %>>
      <h2><%= title %></h2>
    </div>
    <div class="col-sm-4 text-right">
      <% if index == 0 %>
        <%= render "show_actions", presenter: presenter %>
      <% end %>
    </div>
  </div>
<% end %>
